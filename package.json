{
  "name": "data-vision",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "description": "A lightweight and user-empowering Javascript chart library",
  "keywords": [
    "chart",
    "graph",
    "line",
    "bar",
    "donut",
    "gauge",
    "chart library",
    "graph library",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/graphieros/data-viz"
  },
  "homepage": "https://data-vision.graphieros.com/",
  "main": "./dist/data-vision.umd.cjs",
  "module": "./dist/data-vision.js",
  "types": "./dist/index.d.ts",
  "author": "Alec Lloyd Probert",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/data-vision.js",
      "export": "./dist/data-vision.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "dev:build": "vite build --watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.1",
    "@types/node": "^14.18.36",
    "@vitest/coverage-c8": "^0.28.4",
    "happy-dom": "^10.10.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^1.7.2",
    "vitest": "^0.28.4"
  }
}
